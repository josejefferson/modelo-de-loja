{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2020": true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 11
	},
	"rules": {
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"linebreak-style": "off",
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		],
		"no-unused-vars": "off",
		"no-empty": [
			"error",
			{
				"allowEmptyCatch": true
			}
		],
		"no-undef": "off",
		"no-inner-declarations": "off"
	}
}